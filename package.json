{
  "name": "react-universe",
  "version": "0.0.1",
  "description": "Universal React/Redux starter",
  "main": "src/server.js",
  "scripts": {
    "start": "webpack -p && node dist/bundle.js",
    "dev": "NODE_ENV=dev builder concurrent build:watch server:watch",
    "server:watch": "nodemon dist/bundle.js",
    "build:watch": "webpack --watch",
    "clean": "rm -rf dist/ public/ && webpack && npm run dev",
    "flow": "flow check",
    "precommit": "flow check && lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanyesmunt/react-universe.git"
  },
  "author": "seanyesmunt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seanyesmunt/react-universe/issues"
  },
  "homepage": "https://github.com/seanyesmunt/react-universe#readme",
  "lint-staged": {
    "src/**/*.{jsx,js}": [
      "prettier --no-semi --use-tabs --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "express": "^4.15.2",
    "husky": "^0.13.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.2",
    "webpack-node-externals": "^1.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "builder": "^3.2.2",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-babel-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.45.0",
    "lint-staged": "^3.4.1",
    "morgan": "^1.8.1",
    "prettier": "^1.2.2",
    "style-loader": "^0.17.0",
    "webpack": "^2.4.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
